{
    "name": "starapi",
    "version": "8.0.1",
    "description": "StarAPI some silly project tm",
    "main": "app.js",
    "scripts": {
        "prestart": "node scripts/setup.js --check",
        "start": "node server.js",
        "heroku-postbuild": "npm run db:migrate",
        "setup-heroku": "node scripts/setup-heroku.js",
        "build": "npm install && npm run fix-permissions && npm rebuild bcrypt --build-from-source && node scripts/setup.js",
        "dev": "nodemon server.js",
        "setup": "node scripts/setup.js",
        "api": "node app.js",
        "bot": "node bot.js",
        "metrics": "node services/metrics-collector.js",
        "dashboard": "cd dashboard && npm start",
        "build-dashboard": "cd dashboard && npm run build",
        "test": "jest",
        "rebuild": "npm run fix-permissions && npm rebuild bcrypt --build-from-source",
        "fix-permissions": "chmod +x node_modules/.bin/node-pre-gyp",
        "db:migrate": "node scripts/migrate.js"
    },
    "dependencies": {
        "axios": "^1.8.2",
        "bcrypt": "^5.1.1",
        "body-parser": "^1.20.2",
        "cors": "^2.8.5",
        "crypto": "^1.0.1",
        "discord.js": "^14.14.1",
        "dotenv": "^16.4.7",
        "ejs": "^3.1.9",
        "express": "^4.18.2",
        "express-list-endpoints": "^7.1.1",
        "express-rate-limit": "^7.1.5",
        "express-validator": "^7.2.1",
        "helmet": "^7.1.0",
        "jsonwebtoken": "^9.0.2",
        "morgan": "^1.10.0",
        "node-appwrite": "^16.0.0",
        "pg": "^8.11.3",
        "pg-hstore": "^2.3.4",
        "qrcode": "^1.5.3",
        "speakeasy": "^2.0.0",
        "swagger-jsdoc": "^6.2.8",
        "swagger-ui-express": "^5.0.1",
        "systeminformation": "^5.21.24",
        "winston": "^3.17.0"
    },
    "devDependencies": {
        "concurrently": "^9.1.2",
        "nodemon": "^3.0.2"
    },
    "author": "O4bit",
    "license": "MIT",
    "engines": {
        "node": ">=16.0.0",
        "npm": ">=8.0.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/O4bit/StarAPI.git"
    }
}
